{ 
  "api_addr": "https://key-vault.sandbox.net:8200",
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_cert_file": "/vault/tls/key-vault.sandbox.net.crt",
      "tls_key_file": "/vault/tls/key-vault.sandbox.net.key.pem",
      "tls_enable": "true"
    }
  },
  "ui": true
}

